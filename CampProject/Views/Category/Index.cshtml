
@{
    ViewData["Title"] = "Index";
}
@model List<Category>

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Kategori Adı</th>
        <th>Durumu</th>
        <th>Detaylar</th>
        <th>Sil</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.Id</th>
            <th>@item.Name</th>
            <th>@item.Status</th>
            <th><button class="btn btn-primary">Detay</button></th>
            <th><button class="btn btn-warning">Sil</button></th>
        </tr>
    }
</table>
